<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--AutoPhotoLayout-->
    <declare-styleable name="camera_flow_layout">
        <attr name="max_count" format="integer" />
        <attr name="item_margin" format="integer" />
        <attr name="line_count" format="integer" />
        <attr name="icon_size" format="dimension" />
    </declare-styleable>

    <attr name="textSize" format="dimension" />
    <declare-styleable name="IndexBar">
        <attr name="textSize" />
        <attr name="pressBackground" format="color" />
    </declare-styleable>
</resources>